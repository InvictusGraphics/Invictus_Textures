{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/wall/prismarine_wall_post" }
        },
        {   "when": { "north": "low" },
            "apply": { "model": "block/wall/prismarine_wall_side_ns", "uvlock": true }
        },
        {   "when": { "east": "low" },
            "apply": { "model": "block/wall/prismarine_wall_side_ew", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "low" },
            "apply": { "model": "block/wall/prismarine_wall_side_ns", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "low" },
            "apply": { "model": "block/wall/prismarine_wall_side_ew", "y": 270, "uvlock": true }
        },
        {   "when": { "north": "tall" },
            "apply": { "model": "block/wall/prismarine_wall_side_tall", "uvlock": true }
        },
        {   "when": { "east": "tall" },
            "apply": { "model": "block/wall/prismarine_wall_side_tall", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "tall" },
            "apply": { "model": "block/wall/prismarine_wall_side_tall", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "tall" },
            "apply": { "model": "block/wall/prismarine_wall_side_tall", "y": 270, "uvlock": true }
        }
    ]
}
