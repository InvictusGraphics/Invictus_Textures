{
    "multipart": [
        {   "when": { "OR": [
            { "east": true, "west": false },
            { "east": false, "south": false },
            { "north": true, "west": true },
            { "north": false, "south": true }
        ]},
            "apply": { "model": "block/iron_bars_1/iron_bars_post" }
        },
        {   "when": { "north": true, "east": false, "south": true, "west": false },
            "apply": { "model": "block/iron_bars_1/iron_bars_cap_bar" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": true },
            "apply": { "model": "block/iron_bars_1/iron_bars_cap_bar", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "block/iron_bars_1/iron_bars_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/iron_bars_1/iron_bars_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/iron_bars_1/iron_bars_side", "y": 180 }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/iron_bars_1/iron_bars_side", "y": 270 }
        }
    ]
}
